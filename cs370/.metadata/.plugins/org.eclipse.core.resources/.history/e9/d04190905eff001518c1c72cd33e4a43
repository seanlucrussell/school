import java.util.Random;

public class consumer implements Runnable{
	
	circBuffer buffer;
	
	public consumer(circBuffer buffer){
		this.buffer = buffer;
	}

	public void run() {
		Random random = new Random();
		double total = 0;
		int count = 0;
		while (count <= 1000000){
			count ++;
			total += random.nextDouble() * 100.0;
			if(count%100000==0)
				System.out.println("Consumer: " + total);
		}
	}

}
